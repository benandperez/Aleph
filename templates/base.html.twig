                $(document).ready(function(){

                    $('#imageProfile').on('click', function() {
                        $('#employees_imageProfile').trigger('click');
                    });


                    $("#employees_imageProfile").change(function(){
                            var output = document.getElementById('imgPreview');
                            output.src = URL.createObjectURL(event.target.files[0]);
                            output.onload = function() {
                                URL.revokeObjectURL(output.src) // free memory
                            }
                    });
                    $('#imgPreviewUser').on('click', function() {
                        $('#user_profileUser').trigger('click');
                    });

                    $("#user_profileUser").change(function(){
                            var output = document.getElementById('imgUser');
                            output.src = URL.createObjectURL(event.target.files[0]);
                            output.onload = function() {
                                URL.revokeObjectURL(output.src) // free memory
                            }
                    });

                    $('.datetimepicker').each (function(){
                        // $(this).datetimepicker({
                        //     dateFormat: 'dd/mm/yy',
                        // });
                        const str = $(this).attr("value");

                        const [year, month, day] = str && str.split('-');
                        $(this).val(day+"/"+month+"/"+year)

